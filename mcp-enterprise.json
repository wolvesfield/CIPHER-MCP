{
  "mcpServers": {
    "sqlite-audit-db": {
      "command": "uvx",
      "args": ["mcp-server-sqlite", "--db-path", "/absolute/path/to/your/audit_store.db"]
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/absolute/path/to/your/workspace"]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_ENTERPRISE_TOKEN}"
      }
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-playwright"]
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "mcp-chrome-devtools"]
    },
    "browser-devtools-generic": {
      "command": "npx",
      "args": ["-y", "mcp-browser-devtools"]
    },
    "tavily-search": {
      "command": "npx",
      "args": ["-y", "@tavily/mcp-server"],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "supabase-postgres": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres", "${SUPABASE_DB_URL}"]
    },
    "desktop-commander": {
      "command": "npx",
      "args": ["-y", "mcp-desktop-commander"]
    },
    "generic-openapi": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-openapi", "/path/to/generic-swagger-or-openapi.json"]
    },
    "google-places": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-google-maps"],
      "env": {
        "GOOGLE_MAPS_API_KEY": "${GOOGLE_MAPS_API_KEY}"
      }
    },
    "evm-blockchain": {
      "command": "npx",
      "args": ["-y", "@mcpdotdirect/evm-mcp-server"]
    },
    "tatum-blockchain": {
      "command": "npx",
      "args": ["-y", "@tatumio/blockchain-mcp"]
    },
    "identitynow-sailpoint": {
      "command": "npx",
      "args": ["-y", "mcp-sailpoint-identitynow"],
      "env": {
        "SAILPOINT_CLIENT_ID": "${SAILPOINT_CLIENT_ID}",
        "SAILPOINT_CLIENT_SECRET": "${SAILPOINT_CLIENT_SECRET}"
      }
    },
    "garmin-connect": {
      "command": "npx",
      "args": ["-y", "mcp-garmin-connect"],
      "env": {
        "GARMIN_USERNAME": "${GARMIN_USERNAME}",
        "GARMIN_PASSWORD": "${GARMIN_PASSWORD}"
      }
    },
    "dbhub-io": {
      "command": "npx",
      "args": ["-y", "mcp-dbhub"],
      "env": {
        "DBHUB_API_KEY": "${DBHUB_API_KEY}"
      }
    },
    "context7": {
      "command": "node",
      "args": ["/absolute/path/to/your/custom/context7/mcp-server.js"]
    },
    "serena": {
      "command": "node",
      "args": ["/absolute/path/to/your/custom/serena/mcp-server.js"]
    },
    "docker-executor": {
      "command": "npx",
      "args": ["-y", "@0xshariq/docker-mcp-server"]
    },
    "slack-workspace": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-slack"],
      "env": {
        "SLACK_BOT_TOKEN": "${SLACK_BOT_TOKEN}",
        "SLACK_TEAM_ID": "${SLACK_TEAM_ID}"
      }
    },
    "figma-remote": {
      "url": "https://mcp.figma.com/mcp"
    },
    "mem0-memory": {
      "command": "uvx",
      "args": ["mem0-mcp-server"],
      "env": {
        "MEM0_API_KEY": "${MEM0_API_KEY}"
      }
    },
    "python-secure-sandbox": {
      "command": "uvx",
      "args": ["mcp-server-python"]
    },
    "cloudflare-infrastructure": {
      "command": "npx",
      "args": ["-y", "@cloudflare/mcp-server-cloudflare"],
      "env": {
        "CLOUDFLARE_API_TOKEN": "${CLOUDFLARE_API_TOKEN}"
      }
    },
    "github-enterprise-cloud": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_ENTERPRISE_TOKEN}",
        "GITHUB_API_URL": "${GITHUB_API_URL}"
      }
    },
    "kubernetes-cluster": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-kubernetes"],
      "env": {
        "KUBECONFIG": "${KUBECONFIG}"
      }
    },
    "redis-cache": {
      "command": "uvx",
      "args": ["mcp-server-redis", "--url", "${REDIS_URL}"]
    },
    "solana-blockchain": {
      "command": "npx",
      "args": ["-y", "@solana/mcp-server"],
      "env": {
        "SOLANA_RPC_URL": "${SOLANA_RPC_URL}"
      }
    }
  }
}
