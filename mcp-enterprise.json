{
  "mcpServers": {
    "sqlite-audit-db": {
      "command": "uvx",
      "args": ["mcp-server-sqlite", "--db-path", "/absolute/path/to/your/audit_store.db"]
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/absolute/path/to/your/workspace"]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_ENTERPRISE_TOKEN}"
      }
    },
    "playwright": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-playwright"]
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "mcp-chrome-devtools"]
    },
    "browser-devtools-generic": {
      "command": "npx",
      "args": ["-y", "mcp-browser-devtools"]
    },
    "tavily-search": {
      "command": "npx",
      "args": ["-y", "@tavily/mcp-server"],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "supabase-postgres": {
      "command": "npx",
      "args": ["-y", "@supabase/mcp-server-supabase"],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
      }
    },
    "desktop-commander": {
      "command": "npx",
      "args": ["-y", "mcp-desktop-commander"]
    },
    "generic-openapi": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-openapi", "/path/to/generic-swagger-or-openapi.json"]
    },
    "evm-blockchain": {
      "command": "npx",
      "args": ["-y", "@mcpdotdirect/evm-mcp-server"]
    },
    "tatum-blockchain": {
      "command": "npx",
      "args": ["-y", "@tatumio/blockchain-mcp"]
    },
    "dbhub-io": {
      "command": "npx",
      "args": ["-y", "mcp-dbhub"]
    },
    "context7": {
      "command": "node",
      "args": ["/absolute/path/to/your/custom/context7/mcp-server.js"]
    },
    "serena": {
      "command": "node",
      "args": ["/absolute/path/to/your/custom/serena/mcp-server.js"]
    },
    "docker-executor": {
      "command": "npx",
      "args": ["-y", "@0xshariq/docker-mcp-server"]
    },
    "figma-remote": {
      "url": "https://mcp.figma.com/mcp"
    },
    "mem0-memory": {
      "command": "uvx",
      "args": ["mem0-mcp-server"],
      "env": {
        "MEM0_API_KEY": "${MEM0_API_KEY}"
      }
    },
    "python-secure-sandbox": {
      "command": "uvx",
      "args": ["mcp-server-python"]
    },
    "github-enterprise-cloud": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_ENTERPRISE_TOKEN}",
        "GITHUB_API_URL": "${GITHUB_API_URL}"
      }
    },
    "kubernetes-cluster": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-kubernetes"],
      "env": {
        "KUBECONFIG": "${KUBECONFIG}"
      }
    },
    "solana-blockchain": {
      "command": "npx",
      "args": ["-y", "@solana/mcp-server"],
      "env": {
        "SOLANA_RPC_URL": "${SOLANA_RPC_URL}"
      }
    }
  }
}
